{
  "name": "nng/nnhelpers",
  "type": "typo3-cms-extension",
  "license": "GPL-2.0-or-later",
  "description": "A collection of extremely simplified tools and methods to significantly speed up the development and update of Typo3 extensions.",
  "authors": [],
  "require": {
	  "php": ">=8.0",
	  "typo3/cms-core": "^13.0"
  },
  "autoload": {
	  "psr-4": {
	      "Nng\\Nnhelpers\\": "Classes"
	  }
  },
  "autoload-dev": {
	  "psr-4": {
	    "Nng\\Nnhelpers\\Tests\\": "Tests"
	  }
  },
  "extra": {
    "typo3/cms": {
       "extension-key": "nnhelpers"
    }
  }
}